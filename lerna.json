{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "version": "independent",
  "npmClient": "pnpm",
  "useWorkspaces": true,
  "packages": ["packages/*"],
  "command": {
    "version": {
      "tagVersionPrefix": "v",
      "message": "chore(release): publish"
    },
    "publish": {
      "yes": true
    }
  }
}
